{% block javascripts %}
    <script>
        $( document ).ready(function() {
            var secciones=[1,6,11,16,21,26,31,36,41,46,52,56,61,66,71,76,81,86,91,96,101,106,111,116,121];
           var c=0;
            $( "#btnsiguiente" ).click(function() {
                if(secciones[c]==121){
                    $('#btnenviar').show();
                    $('#btnsiguiente').hide();
                }else{
                    $('#seccion'+secciones[c]).hide();
                    c++;
                    $('#seccion'+secciones[c]).show();
                    if(secciones[c]==121){
                        $('#btnenviar').show();
                        $('#btnsiguiente').hide();
                    }
                }
            });
        });
    </script>
